{
  "name": "vite-template-solid",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "yarn eslint && node ./scripts/build.mjs",
    "clean": "rm -rf ./node_modules ./dist ./yarn.lock .yarn-error.log .vscode",
    "eslint": "eslint ./src/**/*",
    "eslint:fix": "yarn eslint --fix",
    "prettier": "prettier --check --ignore-unknown .",
    "prettier:fix": "prettier --write --list-different .",
    "format": "yarn eslint:fix && yarn prettier:fix",
    "serve": "node ./scripts/serve.mjs",
    "dev": "esbuild index.js --outfile=dist_dev/app.js --bundle --sourcemap --watch",
    "dev:file": "node ./scripts/dev.mjs",
    "dev:cli": "mkdir -p ./www && cp ./public/* ./www/* && esbuild --bundle ./src/index.tsx --outdir=./www --servedir=www && rm -rf ./www"
  },
  "license": "MIT",
  "devDependencies": {
    "@craftamap/esbuild-plugin-html": "0.4.0",
    "@typescript-eslint/parser": "^5.29.0",
    "esbuild": "^0.14.47",
    "esbuild-plugin-compress": "^0.3.0",
    "esbuild-plugin-solid": "^0.4.2",
    "esbuild-visualizer": "^0.3.1",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-solid": "^0.7.0",
    "prettier": "^2.7.1",
    "servor": "^4.0.2",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@suid/material": "0.4.3",
    "solid-icons": "0.4.13",
    "solid-js": "1.4.4"
  }
}
