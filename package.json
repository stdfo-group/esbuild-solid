{
  "name": "vite-template-solid",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "yarn eslint && node ./scripts/build.mjs",
    "clean": "rm -rf ./node_modules ./dist .yarn-error.log ./dist_dev",
    "clean:all": "yarn clean ./yarn.lock .vscode yarn-error.log .eslintcache",
    "dev": "node ./scripts/dev.mjs",
    "eslint": "eslint ./src/**/* --cache",
    "eslint:fix": "yarn eslint --fix",
    "format": "yarn eslint:fix && yarn prettier:fix",
    "prettier": "prettier --check --ignore-unknown --cache .",
    "prettier:fix": "prettier --write --list-different --cache .",
    "serve": "node ./scripts/serve.mjs"
  },
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/parser": "^5.29.0",
    "connect": "^3.7.0",
    "esbuild": "^0.14.47",
    "esbuild-plugin-solid": "^0.4.2",
    "esbuild-visualizer": "^0.3.1",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-solid": "^0.7.0",
    "prettier": "^2.7.1",
    "serve-static": "^1.15.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@suid/material": "0.4.3",
    "solid-app-router": "0.4.1",
    "solid-icons": "0.4.13",
    "solid-js": "1.4.4"
  }
}
